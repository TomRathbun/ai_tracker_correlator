{
    "model": {
        "num_sensors": 3,
        "hidden_dim": 32,
        "state_dim": 6,
        "num_heads": 2,
        "edge_dim": 6,
        "emb_dim": 8
    },
    "training": {
        "num_epochs": 100,
        "learning_rate": 0.0005,
        "weight_decay": 1e-05,
        "batch_size": 1,
        "grad_clip": 1.0,
        "use_scheduler": true,
        "scheduler_patience": 10,
        "scheduler_factor": 0.5,
        "early_stopping_patience": 30,
        "save_every": 10,
        "checkpoint_dir": "checkpoints"
    },
    "tracking": {
        "init_thresh_early": 0.15,
        "init_thresh_late": 0.25,
        "coast_thresh_early": 0.05,
        "coast_thresh_late": 0.1,
        "suppress_thresh_early": 1.0,
        "suppress_thresh_late": 0.7,
        "del_exist": 0.01,
        "del_age": 12,
        "track_cap": 50,
        "max_edge_dist": 60000.0,
        "k_neighbors": 10
    },
    "loss": {
        "match_gate": 15000.0,
        "miss_penalty": 5.0,
        "fp_mult_early": 0.5,
        "fp_mult_late": 2.0,
        "pseudo_aux_weight": 1.0,
        "matched_exist_weight": 2.0,
        "logit_reg_weight": 0.001
    },
    "data": {
        "data_file": "data/sim_realistic_003.jsonl",
        "train_split": 0.7,
        "val_split": 0.15,
        "test_split": 0.15,
        "seed": 42
    }
}